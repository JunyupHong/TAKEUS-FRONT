{
  "env": {
    "browser": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    // "arrow-parens": ["error", "as-needed"],
    //아래 경우에 error 처리돼서 주석처리함
    // background: url(${(props) => props.bg}) no-repeat top center;
    // setSelectPage((prev) => prev + 1);
    // 저장할 때 자동으로 ()가 생기는데 해결못함...

    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxBOF": 0,
        "maxEOF": 0
      }
    ],
    "semi": ["error", "always"],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}
